{
  "name": "react-ts-mdl-mobx",
  "version": "1.0.0",
  "description": "React TS MDL MobX",
  "main": "main.txs",
  "scripts": {
    "start": "concurrently \"npm run webpack\" \"node server/index.js\"",
    "start:dev": "concurrently \"npm run webpack\" \"nodemon server/index.js -w server/\"",
    "webpack": "webpack -w"
  },
  "keywords": [
    "react",
    "mdl",
    "mobx",
    "typescript",
    "boilerplate",
    "crud"
  ],
  "author": "Kiho Chang",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^2.2.0",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.5",
    "babel-resolver": "^0.0.18",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mobx-react-devtools": "^4.2.3",
    "nodemon": "^1.9.2",
    "postcss": "^5.1.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.4.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "node-sass": "3.9.0",
    "sass-loader": "4.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "ts-loader": "^0.9.0",
    "typescript": "^2.0.3",
    "whatwg-fetch": "^1.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-addons-create-fragment": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "jade": "^1.11.0",
    "mobx": "^2.4.0",
    "mobx-react": "^3.5.1",
    "react-router": "^2.6.0",
    "react-virtualized": "^7.4.0",
    "react-images": "^0.5.1",
    "es6-promise": "^3.3.0",
    "classnames": "^2.2.3",
    "object-assign": "^4.0.1",
    "pikaday": "~1.4.0",
    "moment": ">= 2.8.0",
    "accounting": "^0.4.1",
    "rx": "^2.5.2",
    "lodash": "^4.15.0",
    "require-async": "^0.0.4"
  },
  "babelConfig": {
    "server": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ]
    },
    "client": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ],
      "env": {
        "development": {
          "plugins": [
            "react-hot-loader/babel"
          ]
        }
      }
    }
  },
  "config": {
    "assetsPublicPath": "/assets/"
  }
}
